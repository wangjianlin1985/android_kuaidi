<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chengxusheji.domain">

	<class name="OrderState" table="OrderState">

          <!-- 主键订单状态id -->
          <id name="orderStateId" column="orderStateId">
            <generator class="native"/>
          </id>

          <!-- 简单字符串属性: 订单状态名称 -->
          <property
            name="orderStateName"
            column="orderStateName"
            type="string"
            length="20"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
